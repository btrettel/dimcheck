&config
output_file     = "src/units.f90"
base_units      = "m", "kg", "s"
type_definition = "real(kind=WP)"
!type_definition = "type(ad)"
use_line        = "use prec, only: WP"
kind_parameter  = "WP"
max_n_units     = 10

min_exponents = 0.0, 0.0, -1.0
max_exponents = 3.0, 0.0, 1.0
denominators  = 2, 1, 2

dtio = .true.
/

&seed_unit
label = "length"
e = 1.0, 0.0, 0.0
/

&seed_unit
label = "time"
e = 0.0, 0.0, 1.0
/

&seed_unit
label = "velocity"
e = 1.0, 0.0, -1.0
/

&seed_unit
label = "area"
e = 2.0, 0.0, 0.0
/

&seed_unit
label = "volume"
e = 3.0, 0.0, 0.0
/
