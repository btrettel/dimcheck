&config
output_file     = "src/units.f90"
base_units      = "m", "kg", "s"
type_definition = "real(kind=WP)"
use_line        = "use prec, only: WP"
kind_parameter  = "WP"
max_n_units     = 15

min_exponents = -3.0, 0.0, -2.0
max_exponents = 3.0, 1.0, 1.0
denominators  = 2, 1, 2

dtio = .true.
/

&seed_unit
label = "unitless"
e = 0.0, 0.0, 0.0
/

&seed_unit
label = "si_length"
e = 1.0, 0.0, 0.0
/

&seed_unit
label = "si_time"
e = 0.0, 0.0, 1.0
/

&seed_unit
label = "si_velocity"
e = 1.0, 0.0, -1.0
/

&seed_unit
label = "si_area"
e = 2.0, 0.0, 0.0
/

&seed_unit
label = "si_volume"
e = 3.0, 0.0, 0.0
/

&seed_unit
label = "si_density"
e = -3.0, 1.0, 0.0
/

&seed_unit
label = "si_frequency"
e = 0.0, 0.0, -1.0
/

&seed_unit
label = "si_energy"
e = 2.0, 1.0, -2.0
/

&seed_unit
label = "si_energy_per_area"
e = 0.0, 1.0, -2.0
/
